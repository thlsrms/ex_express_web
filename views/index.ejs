<!DOCTYPE html>
<html lang="en">
<%- include("./partials/head.ejs") %>
<body>
    <%- include("./partials/nav.ejs") %>
    <div class="content">
        <div id="last-updated">
            <h3>Recently updated: </h3>
            <!-- Last updated -->
            <% if (playlistSearchResult) { %>
                <% playlistSearchResult.forEach(result => { %>
                    <div><p><%= result.name %></p>
                        <p><%= result.author %></p>
                        <p><%= result.url %></p>
                        <p><%= result.updated.getFullYear() + "-" +  (result.updated.getMonth() + 1) + "-" + result.updated.getDate() + " " + result.updated.getHours() + ":" + result.updated.getMinutes() + ":" + result.updated.getSeconds() %></p></div>
                <% }) %>
            <% } %>
        </div>
        <div id="most-accessed">
            <h3>Most accessed: </h3>
        </div>
    </div>
    <%- include("./partials/footer.ejs") %>
</body>
</html>